{
  "manifest_version": 2,
  "name": "My first extensions",
  "version": "1.0.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.5.1.min.js","index.js","background.js"]
    }],


  "browser_action": {
    "default_title": "Launch",
    "default_popup": "index.html"
  }


}